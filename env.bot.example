DEEPGRAM_API_KEY=
CARTESIA_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_SERVICE_ROLE_KEY=
EDGE_API_TOKEN=

# Optional overrides
DAILY_API_KEY = # Add this if you're going to use the Daily transport
TOKEN_USAGE_LOG= # location of token usage metrics #default:"logs/token_usage.csv"
BOT_TEST_CHARACTER_ID= # hardcode character id to join with (used in client testing)
BOT_TEST_CHARACTER_NAME= # hardcoded character name
BOT_TEST_NPC_CHARACTER_NAME= # hardcoded npc name

# Optional: LLM provider configuration (defaults shown)
# Supported providers: google, anthropic, openai, nemotron
VOICE_LLM_PROVIDER=google # requires GOOGLE_API_KEY
VOICE_LLM_MODEL=gemini-2.5-flash
TASK_LLM_PROVIDER=anthropic # requires ANTHROPIC_API_KEY
TASK_LLM_MODEL=claude-sonnet-4-5-20250929
TASK_LLM_THINKING_BUDGET=2048

# UI agent LLM (optional)
UI_AGENT_LLM_PROVIDER=google
UI_AGENT_LLM_MODEL=gemini-2.5-flash
UI_AGENT_LLM_THINKING_BUDGET=0

# Voice STT provider configuration
# Supported providers: cartesia, magpie, nemotron
VOICE_STT_PROVIDER=cartesia
VOICE_STT_LANGUAGE=en

# Cartesia STT options
# CARTESIA_STT_MODEL=ink-whisper
# CARTESIA_STT_BASE_URL=api.cartesia.ai

# Magpie/OpenAI-compatible STT options
# VOICE_STT_PROVIDER=magpie
# NVIDIA_STT_BASE_URL=http://127.0.0.1:8000/v1
# NVIDIA_STT_MODEL=whisper-1
# NVIDIA_STT_API_KEY=empty

# Nemotron websocket ASR options
# VOICE_STT_PROVIDER=nemotron
# NEMOTRON_ASR_URL=ws://127.0.0.1:8080
# NEMOTRON_ASR_MODEL=nvidia/nemotron-speech-streaming-en-0.6b
# NEMOTRON_ASR_SAMPLE_RATE=16000

# Nemotron (vLLM/OpenAI-compatible) example:
# VOICE_LLM_PROVIDER=nemotron
# TASK_LLM_PROVIDER=nemotron
# UI_AGENT_LLM_PROVIDER=nemotron
# VOICE_LLM_MODEL=nvidia/NVIDIA-Nemotron-3-Nano-30B-A3B-BF16
# TASK_LLM_MODEL=nvidia/NVIDIA-Nemotron-3-Nano-30B-A3B-BF16
# UI_AGENT_LLM_MODEL=nvidia/NVIDIA-Nemotron-3-Nano-30B-A3B-BF16
# NEMOTRON_BASE_URL=http://127.0.0.1:8000/v1
# NEMOTRON_API_KEY=empty

# Ships list cache TTL for UI agent (seconds)
UI_AGENT_SHIPS_CACHE_TTL_SECS=60
